[
  {
    "id": "redirect_magnet_links",
    "priority": 1,
    "action": {
      "type": "redirect",
      "redirect": {
        "transform": {
          "scheme": "chrome-extension",
          "host": "@@extension_id@@",
          "path": "/magnet_handler.html",
          "queryTransform": {
            "addOrReplaceParams": [
              {
                "key": "url",
                "value": "magnet:\\1"
              }
            ]
          }
        }
      }
    },
    "condition": {
      "regexFilter": "^magnet:(.*)",
      "resourceTypes": ["main_frame"]
    }
  }
]
